  },
  "dependencies": {
    "mediasoup-client": "^3.16.3",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@webgpu/types": "^0.1.64",
    "concurrently": "^9.2.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": "20.19.4",
    "pnpm": "10.15.0"
  },
  "packageManager": "pnpm@10.15.0"
