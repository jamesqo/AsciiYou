{
  "name": "asciiyou-frontend",
  "version": "1.0.0",
  "description": "WebGPU-based ASCII art webcam application",
  "type": "module",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "dev": "vite --port ${PORT:-8000} --strictPort",
